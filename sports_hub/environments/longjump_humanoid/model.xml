<mujoco model="humanoid_longjump">
  <option timestep="0.002" iterations="2" ls_iterations="8" solver="Newton">
    <flag eulerdamp="disable"/>
  </option>

  <statistic center="0 0 0.1" extent="10.0" meansize="0.04"/>

  <visual>
    <rgba force="1 0 0 1" haze = "0.71 0.71 0.71 0.51" fog="0.71 0.71 0.71 1"/>
    <global azimuth="140" elevation="-20" offwidth="1280" offheight="720"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.71 0.71 0.71" rgb2="0.71 0.71 0.71" width="800" height="800"/>
    <texture name="grey_squares" builtin="checker" height="800" width="800" rgb1="0.71 0.71 0.71" rgb2="0.71 0.71 0.71" type="2d" mark="edge" markrgb="1 1 1"/>
    <texture name="sand" builtin="flat" width="1024" height="1024" rgb1="0.851 0.698 0.412" mark="random" markrgb="0.85 0.75 0.4" random="0.11" />

    <material name="body" rgba=".7 .5 .3 1" specular="0.9" shininess="0.8"/>
    <material name="floor" reflectance="0.2" texrepeat="1 1" texuniform="true" texture="grey_squares"/>
    <material name="sand" texture="sand" texrepeat="100 100" texuniform="false" rgba="1 1 1 1"/>
  </asset>

  <default>
    <geom conaffinity="0" contype="0" condim="3"/>
    <motor ctrllimited="true" ctrlrange="-.4 .4"/>
    <joint armature="1" damping="1" limited="true"/>
    <default class="humanoid">
      <geom type="capsule" material="body"/>
    </default>
  </default>

  <worldbody>
    <geom name="floor" pos="0 0 0" size="100 100 0.010" type="plane" material="floor"/>

    <!-- Humanoid -->
    <body name="humanoid" childclass="humanoid" pos="0 0 1.40">
      <light name="tracking_light" pos="0 0 7" dir="0 0 -1" specular="0.8 0.8 0.8" mode="trackcom"/>
      <camera name="cam_default" mode="trackcom" pos="3.659 -8.426 1.448" xyaxes="0.932 0.363 0.000 -0.075 0.193 0.978"/>
      <site name="humanoid" size="0.01" pos="-0 0 -0"/>
      <joint name="humanoid_root" type="free" armature="0" damping="0" limited="false" pos="0 0 0" stiffness="0"/>
      <geom name="humanoid_torso" fromto="0 -.07 0 0 .07 0" size="0.07" type="capsule"/>
      <geom name="humanoid_head" pos="0 0 .19" size=".09" type="sphere"/>
      <geom name="humanoid_uwaist" fromto="-.01 -.06 -.12 -.01 .06 -.12" size="0.06" type="capsule"/>
      <body name="humanoid_lwaist" pos="-.01 0 -0.260" quat="1.000 0 -0.002 0">
        <geom name="humanoid_lwaist" fromto="0 -.06 0 0 .06 0" size="0.06" type="capsule"/>
        <joint name="humanoid_abdomen_z" armature="0.02" axis="0 0 1" damping="5" pos="0 0 0.065" range="-45 45" stiffness="20" type="hinge"/>
        <joint name="humanoid_abdomen_y" armature="0.02" axis="0 1 0" damping="5" pos="0 0 0.065" range="-75 30" stiffness="10" type="hinge"/>
        <body name="humanoid_pelvis" pos="0 0 -0.165" quat="1.000 0 -0.002 0">
          <joint name="humanoid_abdomen_x" armature="0.02" axis="1 0 0" damping="5" pos="0 0 0.1" range="-35 35" stiffness="10" type="hinge"/>
          <geom name="humanoid_butt" fromto="-.02 -.07 0 -.02 .07 0" size="0.09" type="capsule"/>
          <body name="humanoid_right_thigh" pos="0 -0.1 -0.04">
            <joint name="humanoid_right_hip_x" armature="0.01" axis="1 0 0" damping="5" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
            <joint name="humanoid_right_hip_z" armature="0.01" axis="0 0 1" damping="5" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
            <joint name="humanoid_right_hip_y" armature="0.0080" axis="0 1 0" damping="5" pos="0 0 0" range="-110 20" stiffness="20" type="hinge"/>
            <geom name="humanoid_right_thigh" fromto="0 0 0 0 0.01 -.34" size="0.06" type="capsule"/>
            <body name="humanoid_right_shin" pos="0 0.01 -0.403">
              <joint name="humanoid_right_knee" armature="0.0060" axis="0 -1 0" pos="0 0 .02" range="-160 -2" type="hinge"/>
              <geom name="humanoid_right_shin" fromto="0 0 0 0 0 -.3" size="0.049" type="capsule"/>
              <body name="humanoid_right_foot" pos="0 0 -0.45">
                <geom name="humanoid_right_foot" pos="0 0 0.1" size="0.075" type="sphere"/>
                <site name="humanoid_right_foot" size="0.01" pos="0 0 0.1"/> 
              </body>
            </body>
          </body>
          <body name="humanoid_left_thigh" pos="0 0.1 -0.04">
            <joint name="humanoid_left_hip_x" armature="0.01" axis="-1 0 0" damping="5" pos="0 0 0" range="-25 5" stiffness="10" type="hinge"/>
            <joint name="humanoid_left_hip_z" armature="0.01" axis="0 0 -1" damping="5" pos="0 0 0" range="-60 35" stiffness="10" type="hinge"/>
            <joint name="humanoid_left_hip_y" armature="0.01" axis="0 1 0" damping="5" pos="0 0 0" range="-110 20" stiffness="20" type="hinge"/>
            <geom name="humanoid_left_thigh" fromto="0 0 0 0 -0.01 -.34" size="0.06" type="capsule"/>
            <body name="humanoid_left_shin" pos="0 -0.01 -0.403">
              <joint name="humanoid_left_knee" armature="0.0060" axis="0 -1 0" pos="0 0 .02" range="-160 -2" stiffness="1" type="hinge"/>
              <geom name="humanoid_left_shin" fromto="0 0 0 0 0 -.3" size="0.049" type="capsule"/>
              <body name="humanoid_left_foot" pos="0 0 -0.45">
                <geom name="humanoid_left_foot" type="sphere" size="0.075" pos="0 0 0.1"/>
                <site name="humanoid_left_foot" size="0.01" pos="0 0 0.1"/> 
              </body>
            </body>
          </body>
        </body>
      </body>
      <body name="humanoid_right_upper_arm" pos="0 -0.17 0.06">
        <joint name="humanoid_right_shoulder1" armature="0.0068" axis="2 1 1" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
        <joint name="humanoid_right_shoulder2" armature="0.0051" axis="0 -1 1" pos="0 0 0" range="-85 60" stiffness="1" type="hinge"/>
        <geom name="humanoid_right_uarm" fromto="0 0 0 .16 -.16 -.16" size="0.04 0.16" type="capsule"/>
        <body name="humanoid_right_lower_arm" pos=".18 -.18 -.18">
          <joint name="humanoid_right_elbow" armature="0.0028" axis="0 -1 1" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
          <geom name="humanoid_right_larm" fromto="0.01 0.01 0.01 .17 .17 .17" size="0.031" type="capsule"/>
          <geom name="humanoid_right_hand" pos=".18 .18 .18" size="0.04" type="sphere"/>
        </body>
      </body>
      <body name="humanoid_left_upper_arm" pos="0 0.17 0.06">
        <joint name="humanoid_left_shoulder1" armature="0.0068" axis="2 -1 1" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
        <joint name="humanoid_left_shoulder2" armature="0.0051" axis="0 1 1" pos="0 0 0" range="-60 85" stiffness="1" type="hinge"/>
        <geom name="humanoid_left_uarm" fromto="0 0 0 .16 .16 -.16" size="0.04 0.16" type="capsule"/>
        <body name="humanoid_left_lower_arm" pos=".18 .18 -.18">
          <joint name="humanoid_left_elbow" armature="0.0028" axis="0 -1 -1" pos="0 0 0" range="-90 50" stiffness="0" type="hinge"/>
          <geom name="humanoid_left_larm" fromto="0.01 -0.01 0.01 .17 -.17 .17" size="0.031" type="capsule"/>
          <geom name="humanoid_left_hand" pos=".18 -.18 .18" size="0.04" type="sphere"/>
        </body>
      </body>
    </body>

    <geom name="jump_line" type="box" size="0.1 6 0.005" pos="10 0 0.005" rgba="1 1 1 1" />

    <body name="sand_pit" pos="14 0 0">
      <geom name="sand" type="box" size="20 6 0.05" pos="20 0 0.05" material="sand"/> 
      <geom name="border_front" type="box" size="0.05 6 0.05" pos="-0.05 0 0.05" rgba="1 1 1 1" />
      <geom name="border_back" type="box" size="0.05 6 0.05" pos="40.05 0 0.05" rgba="1 1 1 1" />
      <geom name="border_left" type="box" size="20.1 0.05 0.05" pos="20 6.05 0.05" rgba="1 1 1 1" />
      <geom name="border_right" type="box" size="20.1 0.05 0.05" pos="20 -6.05 0.05" rgba="1 1 1 1" />
    </body>
  </worldbody>

  <contact>
      <!-- Floor contacts -->
    <pair geom1="humanoid_left_foot" geom2="floor" friction="1 1 0.1 0.1 0.1"/>
    <pair geom1="humanoid_right_foot" geom2="floor" friction="1 1 0.1 0.1 0.1"/>
    <!-- <pair geom1="humanoid_torso" geom2="floor" condim="1"/> -->

    <!-- Sand contacts -->
    <pair geom1="humanoid_left_foot" geom2="sand" friction="1 1 0.1 0.1 0.1"/>
    <pair geom1="humanoid_right_foot" geom2="sand" friction="1 1 0.1 0.1 0.1"/>
    <!-- <pair geom1="humanoid_torso" geom2="sand" friction="1 1 0.1 0.1 0.1"/> -->

    <!-- Self contacts -->
    <pair geom1="humanoid_right_foot" geom2="humanoid_left_foot" condim="1"/>
    <pair geom1="humanoid_left_foot" geom2="humanoid_left_thigh" condim="1"/>
    <pair geom1="humanoid_left_foot" geom2="humanoid_butt" condim="1"/>
    <pair geom1="humanoid_left_foot" geom2="humanoid_right_shin" condim="1"/>
    <pair geom1="humanoid_left_foot" geom2="humanoid_right_thigh" condim="1"/>
    <pair geom1="humanoid_right_foot" geom2="humanoid_right_thigh" condim="1"/>
    <pair geom1="humanoid_right_foot" geom2="humanoid_butt" condim="1"/>
    <pair geom1="humanoid_right_foot" geom2="humanoid_left_shin" condim="1"/>
    <pair geom1="humanoid_right_foot" geom2="humanoid_left_thigh" condim="1"/>
    <pair geom1="humanoid_left_shin" geom2="humanoid_right_thigh" condim="1"/>
    <pair geom1="humanoid_right_shin" geom2="humanoid_left_thigh" condim="1"/>
  </contact>

  <actuator>
    <motor gear="100" joint="humanoid_abdomen_y" name="humanoid_abdomen_y"/>
    <motor gear="100" joint="humanoid_abdomen_z" name="humanoid_abdomen_z"/>
    <motor gear="100" joint="humanoid_abdomen_x" name="humanoid_abdomen_x"/>
    <motor gear="100" joint="humanoid_right_hip_x" name="humanoid_right_hip_x"/>
    <motor gear="100" joint="humanoid_right_hip_z" name="humanoid_right_hip_z"/>
    <motor gear="300" joint="humanoid_right_hip_y" name="humanoid_right_hip_y"/>
    <motor gear="200" joint="humanoid_right_knee" name="humanoid_right_knee"/>
    <motor gear="100" joint="humanoid_left_hip_x" name="humanoid_left_hip_x"/>
    <motor gear="100" joint="humanoid_left_hip_z" name="humanoid_left_hip_z"/>
    <motor gear="300" joint="humanoid_left_hip_y" name="humanoid_left_hip_y"/>
    <motor gear="200" joint="humanoid_left_knee" name="humanoid_left_knee"/>
    <motor gear="25" joint="humanoid_right_shoulder1" name="humanoid_right_shoulder1"/>
    <motor gear="25" joint="humanoid_right_shoulder2" name="humanoid_right_shoulder2"/>
    <motor gear="25" joint="humanoid_right_elbow" name="humanoid_right_elbow"/>
    <motor gear="25" joint="humanoid_left_shoulder1" name="humanoid_left_shoulder1"/>
    <motor gear="25" joint="humanoid_left_shoulder2" name="humanoid_left_shoulder2"/>
    <motor gear="25" joint="humanoid_left_elbow" name="humanoid_left_elbow"/>
  </actuator>
</mujoco>

