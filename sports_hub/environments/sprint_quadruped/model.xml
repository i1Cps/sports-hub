
<mujoco model="sprint_quadruped">
  <option timestep="0.004" iterations="1" ls_iterations="5" solver="Newton">
    <flag eulerdamp="disable"/>
  </option>

  <statistic center="0 0 0.1" extent="10.0" meansize="0.04"/>

  <visual>
    <rgba force="1 0 0 1" haze = "0.71 0.71 0.71 0.51" fog="0.71 0.71 0.71 1"/>
    <global azimuth="140" elevation="-20" offwidth="1280" offheight="720"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.71 0.71 0.71" rgb2="0.71 0.71 0.71" width="800" height="800"/>
    <texture name="grass" type="2d" builtin="checker" width="512" height="512" rgb1="0.133 0.745 0.133" rgb2="0.486 0.988 0.0"/>
    <texture name="trunk" type="cube" builtin="flat" width="128" height="128" rgb1="0.545 0.271 0.075" rgb2="0.545 0.271 0.075"/>
    <texture name="foliage" type="cube" builtin="flat" width="128" height="128" rgb1="0.133 0.545 0.133" rgb2="0.133 0.545 0.133"/>
    <texture name="black_white_checkers" type="2d" builtin="checker" width="512" height="512" rgb1="0 0 0" rgb2="1 1 1"/>
    <texture name="orange_brown_checkers" type="2d" builtin="checker" width="512" height="512" rgb1="0.988 0.729 0.012" rgb2="0.918 0.714 0.463"/>

    <material name="body" rgba=".7 .5 .3 1" specular="0.9" shininess="0.8"/>
    <material name="grass" texture="grass" texrepeat="1 1" texuniform="true" reflectance="0.2"/>
    <material name="trunk" texture="trunk" texuniform="true"/>
    <material name="track" texture="orange_brown_checkers" texrepeat="1 1" texuniform="true" reflectance="0.2"/>
    <material name="foliage" texture="foliage" texuniform="true"/>
    <material name="finish_line" texture="black_white_checkers" texrepeat="2 10"/>
  </asset>

  <default>
    <geom conaffinity="0" condim="3" contype="0"/>
    <default class="quadruped">
      <geom type="capsule" material="body" density="5"/>
    </default>
    <joint armature="1" damping="1" limited="true"/>
  </default>

