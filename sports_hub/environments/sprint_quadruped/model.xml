    <geom name="back_railing" type="box" pos="-20 0 0.2" size="0.05 5.05 0.06" rgba="0.9 0.3 0.3 0.61"/>
    <geom name="front_railing" type="box" pos="80 0 0.2" size="0.05 5.05 0.06" rgba="0.9 0.3 0.3 0.61"/>
    <geom name="finish_line" type="box" pos="70 0 0.001" size="1 5 0.001" material="finish_line"/>

    <geom type="plane" name="grass_right" material="grass" pos="30 35 0.150" size="50 30 0.125" rgba="0.2 0.2 0.2 1.0"/>
