    <contact>
      <!-- Floor contacts -->
      <pair geom1="quadruped_front_left_foot" geom2="track" friction="1 1 0.5 0.5 0.5"/>
      <pair geom1="quadruped_front_right_foot" geom2="track" friction="1 1 0.5 0.5 0.5"/>
      <pair geom1="quadruped_back_left_foot" geom2="track" friction="1 1 0.5 0.5 0.5"/>
      <pair geom1="quadruped_back_right_foot" geom2="track" friction="1 1 0.5 0.5 0.5"/>
    </contact>

    <!-- Quadruped has 8 Actuators -->
    <actuator>
      <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="quadruped_front_left_hip" gear="150"/>
      <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="quadruped_front_left_ankle" gear="150"/>
      <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="quadruped_front_right_hip" gear="150"/>
